{
  "name": "e-front",
  "version": "1.0.0",
  "description": "My gnarly Nuxt.js project",
  "author": "amaury-tobias",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build && tsc -p tsconfig.nuxt-config.json",
    "start": "nuxt start -c .nuxt/ts-compiled/nuxt.config.js",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path ../.gitignore .",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@johmun/vue-tags-input": "^2.1.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.0",
    "markdown-it": "^10.0.0",
    "mdi-vue": "^1.6.1",
    "nuxt": "^2.12.2",
    "nuxt-composition-api": "^0.7.4",
    "vue": "^2.6.11",
    "vue-feather-icons": "^5.0.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^1.0.1",
    "@nuxt/typescript-runtime": "^0.4.8",
    "@nuxtjs/color-mode": "^1.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^2.0.0",
    "@teamnovu/nuxt-breaky": "^1.2.1",
    "@types/markdown-it": "^10.0.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": ">=1.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "frontmatter-markdown-loader": "^3.4.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "stylelint": "^13.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss-dark-mode": "^1.1.4",
    "ts-loader": "^7.0.4",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint --fix"
    ]
  }
}
